$TTL 86400
@   IN SOA dns.ent5.local. admin.ent5.local. (
        2024120601 ; Serial (à incrémenter si tu modifies)
        3600        ; Refresh
        1800        ; Retry
        604800      ; Expire
        86400 )     ; Negative Cache TTL

    IN NS dns.ent5.local.

; ---- Serveur DNS ----
dns     IN A 192.168.10.10

; ---- Services de l'entreprise ----
voip    IN A 192.168.10.20
auth    IN A 192.168.10.30
www     IN A 192.168.10.40
files   IN A 192.168.10.50
vpn     IN A 192.168.10.60


